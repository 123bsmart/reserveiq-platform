    "charts.foreground": "#CCCCCC",
    "charts.lines": "#3A3D41",
    "charts.red": "#ef4444",
    "charts.blue": "#3b82f6",
    "charts.yellow": "#f59e0b",
    "charts.orange": "#f97316",
    "charts.green": "#10b981",
    "charts.purple": "#8b5cf6"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6A9955"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Type",
      "scope": ["support.type.primitive"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "JSX Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "JSX Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "Tailwind Classes",
      "scope": ["string.quoted.double.js", "string.quoted.single.js"],
      "settings": {
        "foreground": "#CE9178"
      }
    }
  ]
}
